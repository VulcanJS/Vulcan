# editorconfig.org

root = true

[*.{js,html}]

charset = utf-8
end_of_line = lf
indent_brace_style = 1TBS
indent_size = 2
indent_style = space
insert_final_newline = true
max_line_length = 80
quote_type = auto
spaces_around_operators = true
trim_trailing_whitespace = true

[*.md]

trim_trailing_whitespace = false
