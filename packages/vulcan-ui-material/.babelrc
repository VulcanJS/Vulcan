{
  "plugins": [
    [
      "transform-imports",
      {
        "@material-ui/core": {
          "transform": "@material-ui/core/${member}",
          "preventFullImport": true
        },
        "@material-ui/icons": {
          "transform": "@material-ui/icons/${member}",
          "preventFullImport": true
        },
        "@material-ui/styles": {
          "transform": "@material-ui/styles/${member}",
          "preventFullImport": true
        },
        "lodash": {
          "transform": "lodash/${member}",
          " preventFullImport": true
        }
      }
    ]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "shippedProposals": true,
        "useBuiltIns": "usage",
        "corejs": "2.6.12"
      },
    ],
    "@babel/preset-react"
  ]
}
